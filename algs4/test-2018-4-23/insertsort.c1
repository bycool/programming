#include <stdio.h>

void display(int* arr, int len){
	int i = 0;
	for(i=0;i<len;i++){
		printf("%d,",arr[i]);
	}
	printf("\n");
}

int insertsort(int* arr, int len){
	int i,j, tmp;
	for(i=1;i<len;i++){
		tmp = arr[i];
		j=i;
		while(j>0 && tmp<arr[j-1]){
			arr[j] = arr[j-1];
			j--;
		}
		arr[j] = tmp;
	}
}


int main(){
	int arr[10] = {6,5,4,7,3,9,2,1,0,8};
	//5,6,4,7,3,9,2,1,0,8
	display(arr,10);
	insertsort(arr,10);
	display(arr,10);
	
}
